/**
 * Glyphicons
 */

@font-face {
	font-family: "icomoon";
	
	src: url(glyphs-url + "icomoon.eot");
	src: url(glyphs-url + "icomoon.eot?#iefix") format("embedded-opentype"),
		 url(glyphs-url + "icomoon.woff") format("woff"),
		 url(glyphs-url + "icomoon.ttf") format("truetype"),
		 url(glyphs-url + "icomoon.svg#icomoon") format("svg");
	
	font-weight: normal;
	font-style: normal;
}


.icon-home
.icon-file
.icon-hammer
.icon-compass
.icon-bug
.icon-github
.icon-paragraph-justify
.icon-cog
.icon-chrome
.icon-ie
.icon-firefox
.icon-opera
.icon-safari
	font-family: "icomoon";
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

.icon-home:before
	content: "\21";

.icon-file:before
	content: "\22";

.icon-hammer:before
	content: "\23";

.icon-compass:before
	content: "\24";

.icon-bug:before
	content: "\25";

.icon-github:before
	content: "\26";

.icon-paragraph-justify:before
	content: "\27";

.icon-cog:before
	content: "\28";

.icon-chrome:before
	content: "\29";

.icon-ie:before
	content: "\2a";

.icon-firefox:before
	content: "\2b";

.icon-opera:before
	content: "\2c";

.icon-safari:before
	content: "\2d";