{
	"name": "Drawer",
	"version": "2.0.0",
	"main": ["./src/drawer.js", "./src/jquery.drawer.js"],

	"devDependencies": {
		"google-code-prettify": "1.0.0",
		"angular": "1.2.0-rc.2",
		"angular-route": "1.2.0-rc.2",
		"angular-animate": "1.2.0-rc.2",
		"drawerjs": "2.0.0"
